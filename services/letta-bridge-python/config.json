{
  "redis": {
    "input_queue": "user_input_queue",
    "response_channel": "letta_responses",
    "host": "localhost",
    "port": 6379
  },
  "letta": {
    "api_url": "http://localhost:8283",
    "api_key": "",
    "timeout": 30,
    "retry_attempts": 3,
    "retry_delay": 1
  },
  "bridge": {
    "process_memory_types": ["permanent"],
    "fallback_to_memory": true,
    "max_concurrent_requests": 10,
    "agent_creation_timeout": 60
  },
  "session": {
    "agent_mapping_key": "letta:user_agent_mapping",
    "session_ttl_hours": 24
  },
  "logging": {
    "level": "INFO"
  }
}